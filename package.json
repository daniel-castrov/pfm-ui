    "prettier": "^1.19.1",
    "ts-loader": "^6.2.1",
    "ts-node": "~8.2.0",
    "tsickle": "^0.37.1",
    "tslint": "~5.17.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.3.10"
  }
