{
  "name": "pfm-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "prod": "ng serve -o -c production",
    "local": "ng serve -o -c local",
    "build": "ng build",
    "test": "jest --coverage --logHeapUsage -w=2 --config src/test/jest.conf.js",
    "lint": "ng lint",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run angular-plugin-architecture:server:production",
    "build:planning-create": "ng build --project plugins --prod --modulePath=./app/planning/create-planning/create-planning.module#CreatePlanningModule --pluginName=planning-create --outputPath=./src/assets/plugins"
