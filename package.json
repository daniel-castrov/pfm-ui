{
  "name": "angular-plugin-architecture",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "lint": "ng lint",
    "postinstall": "tsc -p builders/tsconfig.builders.json",
    "build:shared": "ng build shared && ng build --project plugins --prod --modulePath=shared#SharedModule --pluginName=shared  --outputPath=./src/assets/plugins",
    "build:budget-core": "ng build budget-core && ng build --project plugins --prod --modulePath=budget-core#BudgetCoreModule --pluginName=budget-core  --outputPath=./src/assets/plugins",
    "build:planning-core": "ng build planning-core && ng build --project plugins --prod --modulePath=planning-core#PlanningCoreModule --pluginName=planning-core  --outputPath=./src/assets/plugins",
    "build:programming-core": "ng build programming-core && ng build --project plugins --prod --modulePath=programming-core#ProgrammingCoreModule --pluginName=programming-core  --outputPath=./src/assets/plugins",
    "build:budget": "ng build --project plugins --prod --modulePath=./budget/budget.module#BudgetModule --pluginName=budget --sharedLibs=shared --outputPath=./src/assets/plugins",
    "build:budget-demo": "ng build --project plugins --prod --modulePath=./budget-demo/budget-demo.module#BudgetDemoModule --pluginName=budget-demo --sharedLibs=shared --outputPath=./src/assets/plugins",
    "build:planning": "ng build --project plugins --prod --modulePath=./planning/planning.module#PlanningModule --pluginName=planning --sharedLibs=shared --outputPath=./src/assets/plugins ",
    "build:programming": "ng build --project plugins --prod --modulePath=./programming/programming.module#ProgrammingModule --pluginName=programming --sharedLibs=shared --outputPath=./src/assets/plugins ",
    "build:plugins": "npm run build:shared && npm run build:budget-core && npm run build:budget-demo && npm run build:planning-core && npm run build:programming-core && npm run build:budget && npm run build:planning && npm run build:programming",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run angular-plugin-architecture:server:production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.10",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.10",
    "@angular/compiler": "~8.2.10",
    "@angular/core": "~8.2.10",
    "@angular/forms": "~8.2.10",
    "@angular/http": "~7.2.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.10",
    "@angular/platform-browser-dynamic": "~8.2.10",
    "@angular/platform-server": "~8.2.10",
    "@angular/router": "~8.2.10",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "7.1.1",
    "ag-grid": "^18.1.2",
    "angular-font-awesome": "^3.1.2",
    "bootstrap": "^4.3.1",
    "core-js": "^3.1.3",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "ng2-google-charts": "^4.0.0",
    "popper": "^1.0.1",
    "rxjs": "~6.5.3",
    "systemjs": "3.1.6",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.8",
    "@angular-devkit/build-ng-packagr": "~0.803.8",
    "@angular/cli": "~8.3.8",
    "@angular/compiler-cli": "~8.2.10",
    "@angular/language-service": "~8.2.10",
    "@types/node": "~12.0.4",
    "codelyzer": "^5.0.0",
    "karma": "~4.1.0",
    "ng-packagr": "^5.6.1",
    "prettier": "^1.16.4",
    "ts-loader": "^6.0.2",
    "ts-node": "~8.2.0",
    "tsickle": ">=0.34.0",
    "tslint": "~5.17.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.1.0"
  }
}
