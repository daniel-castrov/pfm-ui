{
  "name": "angular-plugin-architecture",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "local": "ng serve -o -c local",
    "build": "ng build",
    "lint": "ng lint",
    "postinstall": "tsc -p builders/tsconfig.builders.json",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run angular-plugin-architecture:server:production",
    "build:planning-create": "ng build --project plugins --prod --modulePath=./app/planning/create-planning/create-planning.module#CreatePlanningModule --pluginName=planning-create --outputPath=./src/assets/plugins"
  },
  "private": true,
  "dependencies": {
    "@ag-grid-community/all-modules": "^22.1.1",
    "@ag-grid-community/angular": "^22.1.2",
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/http": "~7.2.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ng-bootstrap/ng-bootstrap": "^5.1.4",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "7.1.1",
    "@types/jest": "^24.0.25",
    "angular-font-awesome": "^3.1.2",
    "angular-gridster2": "^8.2.0",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.2",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "ng2-file-upload": "^1.4.0",
    "ng2-google-charts": "^4.0.0",
    "ngx-bootstrap": "^5.2.0",
    "popper": "^1.0.1",
    "rxjs": "^6.5.4",
    "systemjs": "3.1.6",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.21",
